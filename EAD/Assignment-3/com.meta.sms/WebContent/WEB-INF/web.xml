<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Student Management System</display-name>
  
  <servlet>
  	<servlet-name>AddStudent</servlet-name>
    <servlet-class>com.meta.sms.AddStudent</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>AddStudent</servlet-name>
  	<url-pattern>/AddStudent</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>SearchStudentByClass</servlet-name>
    <servlet-class>com.meta.sms.SearchStudentByClass</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>SearchStudentByClass</servlet-name>
  	<url-pattern>/SearchStudentByClass</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>SearchStudentByName</servlet-name>
    <servlet-class>com.meta.sms.SearchStudentByName</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>SearchStudentByName</servlet-name>
  	<url-pattern>/SearchStudentByName</url-pattern>
  </servlet-mapping>
  
  
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
</web-app>